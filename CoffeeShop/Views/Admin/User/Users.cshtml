@model List<Microsoft.AspNetCore.Identity.IdentityUser>

@{
    ViewData["Title"] = "Danh sách người dùng";
    Layout = "~/Views/Shared/_AdminLayout.cshtml"; // Hoặc đúng đường dẫn tới layout Admin
}

<h2>@ViewData["Title"]</h2>
<hr />

<table class="table table-striped">
    <thead>
        <tr>
            <th>Email</th>
            <th>UserName</th>
            <th>Id</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>@user.UserName</td>
                <td>@user.Id</td>
            </tr>
        }
    </tbody>
</table>
